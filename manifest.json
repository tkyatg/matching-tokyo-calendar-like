{
  "name": "東カレ自動いいね",
  "description": "東カレで自動いいねを送信!",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "東カレ自動いいねツール",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://tokyo-calendar-date.jp/search/list*"],
      "js": ["like.js","jquery-3.4.0.min.js"]
    }
  ]
}
